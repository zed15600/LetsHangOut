Rails.application.routes.draw do
  get 'welcome/index'
  get 'personas/index'
  get 'personas/show'
  get 'personas/new'
  get 'personas/edit'
  get 'personas/create'
  get 'personas/update'
  get 'personas/destroy'
  # For details on the DSL available within this file, see http://guides.rubyonrails.org/routing.html
end
